{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/node_modules": true,
    "**/dist": true,
    "**/.build": true,
    "coverage": true,
    ".webpack": true,
    "**/.serverless": true,
    "**/_warmup": true
  },
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/**": true,
    "**/env/**": true,
    "**/venv/**": true,
    "**/dics/**": true,
    ".webpack": true,
    "env-*": true
  },
  "search.exclude": {
    ".webpack": true,
    "**/dist": true,
    "**/docs/**": true,
    "**/node_modules": true
  },
  "git.ignoreLimitWarning": true,
  "git.enabled": false,
  "git.path": null,
  "git.autofetch": false,
  "cSpell.words": ["nrwl"],
  "cSpell.ignoreWords": ["awsconfig", "datepicker", "formik", "insite", "signup", "zipcode"]
}
